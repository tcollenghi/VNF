<script type="text/javascript">
    function logout() {
        $.ajax({
            url: '@Url.Action("Logout", "User")',
            type: 'GET',
            dataType: 'text',
            cache: false,
            async: false,
            success: function (data) {
                if (data != null) {
                    window.location = '@Url.Action("Login", "User")';
                }
            }
        });
    }
</script>